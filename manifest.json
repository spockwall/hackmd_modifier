{
	"manifest_version": 4,
	"name": "hackmd_modifier",
	"description": "This extension is used to fix hackmd's compile problem",
	"version": "2.0.0",
	"permissions": ["contextMenus", "tabs"],
	"content_scripts": [
		{
			"matches": ["https://hackmd.io/*"],
			"js": ["Content.js"],
			"run_at": "document_end"
		}
	],
	"background": {
		"service_worker": "event.js"
	},
	"icons": { "16": "/assets/icon.png" }
}
