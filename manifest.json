{
	"manifest_version": 3,
	"name": "hackmd_modifier",
	"version": "1.2.0",
	"content_scripts": [
		{
			"matches": ["https://hackmd.io/*"],
			"js": ["Content.js"],
			"run_at": "document_end"
		}
	],
	"browser_action": {
		"default_popup": "Popup.html",
		"default_title": "fuck"
	},
	"icons": { "16": "/assets/icon.png" }
}
